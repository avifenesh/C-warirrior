{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Code Warrior",
  "version": "0.1.0",
  "identifier": "com.codewarrior.app",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run --prefix ../src-ui build",
    "frontendDist": "../src-ui/build"
  },
  "app": {
    "windows": [
      {
        "title": "Code Warrior",
        "width": 1400,
        "height": 900,
        "minWidth": 1200,
        "minHeight": 800,
        "resizable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [],
    "resources": {
      "../src-ui/static/sprites/*": "sprites/",
      "../src-ui/static/tiles/*": "tiles/",
      "../src-ui/static/ui/*": "ui/"
    }
  }
}
